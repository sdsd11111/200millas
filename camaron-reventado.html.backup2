                            transform: none;
                            background: transparent;
                            max-height: 0;
                            overflow: hidden;
                            transition: max-height 0.4s ease, padding 0.4s ease;
                            margin-top: 0;
                        }

                        .dropdown.active .dropdown-menu {
                            display: block;
                            max-height: 1000px;
                            padding: 0.5rem 0 0.5rem 1rem;
                        }


            `;

            // El header se inicializa automáticamente con load-header.js
        }
            const mobileMenu = document.getElementById('mobile-menu');
            const navLinks = document.querySelector('.nav-links');
            const dropdowns = document.querySelectorAll('.dropdown');

            if (!header || !mobileMenu || !navLinks) return;

            // Efecto de scroll
            window.addEventListener('scroll', function() {
                if (window.scrollY > 50) {
                    header.classList.add('scrolled');
                } else {
                    header.classList.remove('scrolled');
                }
            });

            // Menú móvil
            mobileMenu.addEventListener('click', function(e) {
                e.stopPropagation();
                this.classList.toggle('active');
                navLinks.classList.toggle('active');
                document.body.classList.toggle('menu-open');
            });

            // Dropdown para móvil
            dropdowns.forEach(dropdown => {
                const toggle = dropdown.querySelector('.dropdown-toggle');

                if (toggle) {
                    toggle.addEventListener('click', function(e) {
                        if (window.innerWidth <= 992) {
                            e.preventDefault();
                            e.stopPropagation();
                            dropdown.classList.toggle('active');
                        }
                    });

                    // Cerrar menú al hacer clic en un enlace
                    dropdown.querySelectorAll('a').forEach(link => {
                        link.addEventListener('click', function() {
                            if (window.innerWidth <= 992) {
                                mobileMenu.classList.remove('active');
                                navLinks.classList.remove('active');
                                document.body.classList.remove('menu-open');
                            }
                        });
                    });
                }
            });

            // Cerrar menú al hacer clic fuera
            document.addEventListener('click', function(e) {
                if (window.innerWidth <= 992 &&
                    !e.target.closest('.navbar') &&
                    navLinks.classList.contains('active')) {
                    mobileMenu.classList.remove('active');
                    navLinks.classList.remove('active');
                    document.body.classList.remove('menu-open');
                }
            });

            // Cerrar menú al cambiar el tamaño de la ventana
            window.addEventListener('resize', function() {
                if (window.innerWidth > 992) {
                    mobileMenu.classList.remove('active');
                    navLinks.classList.remove('active');
                    document.body.classList.remove('menu-open');

                    // Cerrar todos los dropdowns en desktop
                    dropdowns.forEach(dropdown => {
                        dropdown.classList.remove('active');
                    });
                }
            });

            console.log('✅ Header initialized successfully');
        }

        // Función para manejar la carga de la página
        function handlePageLoad() {
            // Agregar clase de carga al body
            document.body.classList.add('loading');
            
            // Cargar el header
            if (document.readyState === 'loading') {
                document.addEventListener('DOMContentLoaded', loadHeader);
            } else {
                loadHeader();
            }
            
            // Esperar a que cargue todo el contenido
            window.addEventListener('load', function() {
                // Pequeño retraso para asegurar que todo esté listo
                setTimeout(function() {
                    // Ocultar el loader
                    const loadingScreen = document.getElementById('loading-screen');
                    if (loadingScreen) {
                        loadingScreen.style.opacity = '0';
                        loadingScreen.style.visibility = 'hidden';
                        
                        // Mostrar el contenido
                        document.body.classList.remove('loading');
                        document.body.classList.add('loaded');
                        
                        // Eliminar el loader del DOM después de la animación
                        setTimeout(function() {
                            loadingScreen.remove();
                        }, 500);
                    }
                }, 300); // Tiempo más corto que en la página de inicio (300ms vs 500ms)
            });
        }
        
        // Iniciar la carga de la página
        handlePageLoad();
    </script>

